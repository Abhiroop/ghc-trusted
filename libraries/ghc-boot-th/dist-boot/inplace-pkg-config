    /home/abhir00p/ghc-8.8.2/libraries/ghc-boot-th/dist-boot/build

dynamic-library-dirs:
    /home/abhir00p/ghc-8.8.2/libraries/ghc-boot-th/dist-boot/build

data-dir:             /home/abhir00p/ghc-8.8.2/libraries/ghc-boot-th
hs-libraries:         HSghc-boot-th-8.8.2
depends:              base-4.12.0.0
